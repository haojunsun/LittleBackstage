    <script type="text/javascript">
        var inter1, inter2;
        var indexrem;

        //民族展开
        function minzudivopen() {
            var spanval = $('#minzuspan').attr('_stats');
            var e = document.getElementById('minzudiv');
            if (spanval == 'over') {
                e.style.height = '196px';
                $('#minzuspan').attr('_stats', 'open');
                $('#minzuspan').attr('src', '../img/shut.png');
                $('#searchdiv').css('margin-top', '5px');
            } else if (spanval == 'open') {
                e.style.height = '60px';
                $('#minzuspan').attr('_stats', 'over');
                $('#minzuspan').attr('src', '../img/open.png');
                $('#searchdiv').css('margin-top', '5px');
            }
        }

        $(document).ready(function () {
            //默认隐藏div
            setTimeout(function () {
                $('#shadowdiv').css('opacity', '0.9');
            }, 1000);


            //设置标题居中
            var e = document.getElementById('topimgdiv');
            var win = document.documentElement || document.body;
            var width = e.clientWidth;
            var windowWidth = win.clientWidth;
            var leftoffset = (windowWidth - width) / 2;
            e.style.left = leftoffset + 'px';

            //
            setInterval(function () {
                if ($('.jplist-label').hasClass('jplist-empty') || $('#shadowdiv').hasClass('jplist-hidden')) {
                    $('body').css('height', '1300px');
                    $('#pagediv').css('opacity', '0');
                } else {
                    $('body').css('height', '2275px');
                    $('#pagediv').css('opacity', '1');
                }
            }, 200);

            $('#s1').keyup(function () {
                if ($(this).val() == '') {
                    setTimeout(function () {
                        $('#shadowdiv').addClass('jplist-hidden');
                    }, 0);
                }
            })

            //头部切换搜索
            //头部搜索框
            //切换操作
            $('#search-bd li').click(function () {
                var index = $(this).index();//0
                if (indexrem == index)
                    return;
                indexrem = index;
                //index == 2 ? $('#search-hd .pholder').hide(0) : $('#search-hd .pholder').eq(index).show().siblings('.pholder').hide(0);
                //隐藏其他搜索框
                if (index == 0) {
                    $('#s1div').css('display', 'block');
                    $('#s2div').css('display', 'none');
                } else if (index == 1) {
                    $('#s1div').css('display', 'none');
                    $('#s2div').css('display', 'block');
                }

                //li选中样式
                $(this).addClass('selected').siblings().removeClass('selected');
                //清空搜索框
                //$('#search-hd .search-input').val('');
                //高级搜索 shadowdiv pagediv
                if (index == 1) {
                    $('.jplist-reset-btn').click();//重置
                    var e = document.getElementById('search-hd');
                    var hei = e.clientHeight;
                    clearInterval(inter2);
                    inter1 = setInterval(function () {
                        e.style.height = e.getBoundingClientRect().height + 9 + "px";
                        if (e.getBoundingClientRect().height >= 165) {
                            clearInterval(inter1);
                        }
                    }, 10);
                    $('#shadowdiv').css('margin-top', '136px');

                    $('#shadowdiv').css('opacity', '0');
                    $('#shadowdiv').addClass('jplist-hidden');
                    setTimeout(function () {
                        $('#shadowdiv').css('opacity', '0.9');
                    }, 500);

                } else {//切换全文搜索
                    $('.jplist-reset-btn').click();//重置
                    var e = document.getElementById('search-hd');
                    var hei = e.clientHeight;
                    clearInterval(inter1);
                    inter2 = setInterval(function () {
                        e.style.height = e.getBoundingClientRect().height - 12 + "px";
                        if (e.getBoundingClientRect().height <= 53) {
                            e.style.height = '62px';
                            clearInterval(inter2);
                        }
                    }, 10)

                    setTimeout(function () {
                        $('#shadowdiv').css('margin-top', '80px');
                    }, 300);

                    $('#shadowdiv').css('opacity', '0');
                    $('#shadowdiv').addClass('jplist-hidden');
                    setTimeout(function () {
                        $('#shadowdiv').css('opacity', '0.9');
                    }, 500);
                }
            });
            $('#search-hd .search-input').eq(0).show().siblings('.search-input').hide(0);

        })

        //落叶动画
        $.AutomLeafStart({
            leafsfolder: "../images/",
            howmanyimgsare: 8,
            initialleafs: 10,
            maxYposition: -25,
            multiplyclick: false,
            multiplynumber: 2,
            infinite: true,
            fallingsequence: 2000
        }); $("#botAgregar").on("click", function () { $.AutomLeafAdd({ leafsfolder: "~/images/", add: 7, }) });
    </script>
